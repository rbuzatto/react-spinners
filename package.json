{
    "name": "react-spinners",
    "version": "1.0.0",
    "main": "index.js",
    "repository": "git@github.com:rbuzatto/react-spinners.git",
    "author": "rbuzatto <mightyrafa@gmail.com>",
    "keywords": [
        "react",
        "css",
        "spinners"
    ],
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-Staged": {
